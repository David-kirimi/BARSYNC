{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/dist/assets/$1"
    },
    {
      "source": "/manifest.json",
      "destination": "/dist/manifest.json"
    },
    {
      "source": "/sw.js",
      "destination": "/dist/sw.js"
    },
    {
      "source": "/icon-(.*).png",
      "destination": "/dist/icon-$1.png"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}